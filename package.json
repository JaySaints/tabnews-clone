{
  "name": "tabnews-clone",
  "version": "1.0.0",
  "description": "[curso.dev]: Reconstrução do site https://www.tabnews.com.br/",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:down": "docker compose -f infra/compose.yaml down",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaySaints/tabnews-clone.git"
  },
  "keywords": [
    "NodeJS",
    "NextJS"
  ],
  "author": "Pablo J. Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JaySaints/tabnews-clone/issues"
  },
  "homepage": "https://github.com/JaySaints/tabnews-clone#readme",
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^2.8.8"
  }
}
